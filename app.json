{
  "expo": {
    "name": "IQ, Mental, Personality Tests",
    "slug": "iq-mental-personality-tests",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/brain.png",
    "scheme": "IQ, Mental, Personality Tests",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.alperen.cat",
      "version": "2",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/educating.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
